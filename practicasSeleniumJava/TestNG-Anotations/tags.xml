<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite de Pruebas">



  <test thread-count="5" name="Casos de prueba anotacion">
  <groups>
	<run><include name="functional"></include>
	</run></groups>
	
    <classes>
      <class name="anotations.AnotationesBase"/>
      <class name="anotations.SegundoSet"/>
    </classes>
  </test> <!-- Casos de prueba anotacion -->
  
  
  <test  name="Casos de prueba nuevos">
   <groups>
	<run><include name="smoke"></include>
	</run></groups>
	
    <classes>
      <class name="anotations.TercerSet"/>
    </classes>
  </test>
  
  
</suite> <!-- Suite de Pruebas -->
